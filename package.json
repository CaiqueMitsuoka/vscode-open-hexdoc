{
	"name": "open-hexdoc",
	"repository": {
		"type": "git",
		"url": "https://github.com/w-sanches/vscode-open-hexdoc"
	},
	"publisher": "w-sanches",
	"displayName": "Open HexDoc",
	"description": "",
	"version": "0.0.5",
	"engines": {
		"vscode": "^1.44.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"workspaceContains:./mix.lock"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "open-hexdoc.openHexDoc",
				"title": "Open HexDoc"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"dependencies": {
		"got": "^11.1.1"
	},
	"devDependencies": {
		"@types/vscode": "^1.44.0",
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.11.0",
		"eslint": "^6.8.0",
		"@typescript-eslint/parser": "^2.30.0",
		"@typescript-eslint/eslint-plugin": "^2.30.0",
		"glob": "^7.1.6",
		"mocha": "^7.1.2",
		"typescript": "^3.8.3",
		"vscode-test": "^1.3.0"
	}
}
